{
    "manifest_version": 3,
    "name": "NordSales Extension",
    "version": "1.0.0",
    "description": "Chrome extension built with React and Vite",
    "action": {
      "default_popup": "src/pages/popup/index.html"
    },
    "options_page": "src/pages/options/index.html",
    "background": {
      "service_worker": "src/service-worker.js"
    },
    "permissions": ["storage"],
      "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
  }